{
  "type": "publisher",
  "name": "File Publisher",
  "package": "geoPkg",
  "host": "python",
  "contexts": [
    {
      "name": "context selector",
      "plugin": "context.publish",
      "widget": "context.publish",
      "options":{
        "context_id": null
      }
    }
  ],
  "components": [
    {
      "name": "main",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "collect from given path",
              "plugin": "filesystem",
              "options": {
                "path": null
              }
            }
          ]
        },
        {
          "name":"validator",
          "plugins": [
            {
              "name": "validate selection",
              "plugin": "nonempty"
            }
          ]
        },
        {
          "name":"output",
          "plugins": [
            {
              "name": "to temp",
              "plugin": "to_tmp"
            }
          ]
        }
      ]
    }
  ],
  "finalisers": [
    {
      "name": "to ftrack server",
      "plugin": "result",
      "visible": false
    }
  ]
}