{
  "type": "publisher",
  "name": "Comp Publisher",
  "package": "compPackage",
  "host": "nuke",
  "ui": "qt",
  "contexts": {
      "name": "context",
      "plugins":[
        {
          "name": "context selector",
          "plugin": "context.publish",
          "widget": "context.publish"
        }
      ]
    },
  "components": [
    {
      "name": "script",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "from scene",
              "plugin": "from_scene",
              "options": {
                "node_type": "write"
              }
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "non_empty"
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "nuke file",
              "plugin": "nuke_file",
              "editable": false,
              "options": {
                "file_type": "nk"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "cache",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "from nodes",
              "plugin": "from_nodes",
              "options": {
                "node_type": "geo"
              }
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "non_empty"
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "alembic",
              "plugin": "geometry",
              "editable": false,
              "options": {
                "file_type": "abc"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "beauty",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "from prefix",
              "plugin": "from_prefix",
              "options": {
                "suffix": "beauty_"
              }
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "non_empty"
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "render pass",
              "plugin": "image",
              "options": {
                "file_type": "exr",
                "color_depth": 16
              }
            }
          ]
        }
      ]
    },
    {
      "name": "diffuse",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "from prefix",
              "plugin": "from_prefix",
              "options": {
                "prefix": "diff_"
              }
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "non_empty"
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "render pass",
              "plugin": "image",
              "options": {
                "file_type": "exr",
                "color_depth": 16
              }
            }
          ]
        }
      ]
    },
    {
      "name": "reflection",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "from prefix",
              "plugin": "from_prefix",
              "options": {
                "prefix": "ref_"
              }
            }
          ]
        },
        {
          "name": "validate",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "non_empty"
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "render pass",
              "plugin": "image",
              "options": {
                "file_type": "exr",
                "color_depth": 16
              }
            }
          ]
        }
      ]
    },
    {
      "name": "shadow",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "from prefix",
              "plugin": "from_prefix",
              "options": {
                "prefix": "shad_"
              }
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "non_empty"
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "render pass",
              "plugin": "image",
              "options": {
                "file_type": "exr",
                "color_depth": 16
              }
            }
          ]
        }
      ]
    },
    {
      "name": "specular",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "from prefix",
              "plugin": "from_prefix",
              "options": {
                "prefix": "spec_"
              }
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "non_empty"
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "render pass",
              "plugin": "image",
              "options": {
                "file_type": "exr",
                "color_depth": 16
              }
            }
          ]
        }
      ]
    },
    {
      "name": "reviewable",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "from scene",
              "plugin": "scene"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "non_empty"
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "playblast",
              "plugin": "playblast",
              "options": {
                "from_camera": "persp",
                "start_frame": 0,
                "end_frame": 100,
                "encoding": "h264",
                "file_type": "mov"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "thumbnail",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "from viewport",
              "plugin": "from_viewport"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "non_empty"
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "thumbnail",
              "plugin": "image",
              "options": {
                "file_type": "jpg"
              }
            }
          ]
        }
      ]
    }
  ],
  "finalisers": {
      "name": "finaliser",
      "plugins":[
        {
          "name": "to ftrack server",
          "plugin": "to_ftrack",
          "visible": false
        }
      ]
    }
}