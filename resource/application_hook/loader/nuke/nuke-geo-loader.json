{
  "type": "loader",
  "name": "nukeGeoLoad",
  "package": "geoPkg",
  "host": "nuke",
  "ui": "qt",
  "contexts": {
      "name": "context",
      "plugins":[
        {
          "name": "context selector",
          "plugin": "context.load",
          "widget": "context.load"
        }
      ]
    },
  "components": [
    {
      "name": "cache",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "Collect components from context",
              "plugin": "collect_from_context",
              "options": {
                "component_list": ["cache"],
                "accepted_formats": [".abc"]
              }
            }
          ]
        },
        {
          "name": "importer",
          "plugins":[
            {
              "name": "Import abc",
              "plugin": "abc_nuke_import"
            }
          ]
        },
        {
          "name": "post_import",
          "plugins":[
            {
              "name": "nuke",
              "plugin": "postImportTest"
            }
          ]
        }
      ]
    }
  ],
  "finalizers": {
      "name": "finalizer",
      "plugins":[
        {
          "name": "Do nothing",
          "plugin": "finalizerTest",
          "visible": false
        }
      ]
    }
}