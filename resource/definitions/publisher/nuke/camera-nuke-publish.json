{
  "type": "publisher",
  "name": "Camera Publisher",
  "asset_type": "cam",
  "host_type": "nuke",
  "ui_type": "qt",
  "contexts": [
    {
      "name": "main",
      "stages": [
        {
          "name": "context",
          "plugins":[
            {
              "name": "context selector",
              "plugin": "context.publish",
              "widget": "context.publish"
            }
          ]
        }
      ]
    }
  ],
  "components": [
     {
      "name": "cache",
      "optional": true,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "Selection collector",
              "plugin": "selection"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "node_type",
              "options" : {
                "node_type": "Camera"
              }
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "Export camera",
              "plugin": "camera",
              "options": {
                  "file_type": "abc"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "game",
      "optional": true,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "Selection collector",
              "plugin": "selection"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "node_type",
              "options" : {
                "node_type": "Camera"
              }
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "Export camera",
              "plugin": "camera",
              "options": {
                  "file_type": "fbx"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "geo",
      "optional": true,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "Selection collector",
              "plugin": "selection"
            }
          ]
        },
        {
          "name": "validator",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "node_type",
              "options" : {
                "node_type": "Camera"
              }
            }
          ]
        },
        {
          "name": "output",
          "plugins":[
            {
              "name": "Export camera",
              "plugin": "camera",
              "options": {
                  "file_type": "obj"
              }
            }
          ]
        }
      ]
    }
    ],
  "finalizers": [
    {
      "name": "main",
      "stages": [
        {
          "name": "pre_finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "pre to ftrack server",
              "plugin": "pre_result"
            }
          ]
        },
        {
          "name": "finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "to ftrack server",
              "plugin": "result.nuke"
            }
          ]
        },
        {
          "name": "post_finalizer",
          "visible": false,
          "plugins":[
            {
              "name": "post to ftrack server",
              "plugin": "post_result"
            }
          ]
        }
      ]
    }
  ]
}