{
  "type": "loader",
  "name": "File Loader",
  "package": "pythonPkg",
  "host": "python",
  "contexts": {
      "name": "context",
      "plugins":[
        {
          "name": "context selector",
          "plugin": "context.load",
          "widget": "context.load"
        }
      ]
    },
  "components": [
    {
      "name": "main",
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "Pick objects with prefix",
              "plugin": "collectorTest"
            },
            {
              "name": "Collect components from context",
              "plugin": "collect_from_context",
              "options": {
                "accepted_formats": [".mb"]
              }
            }
          ]
        },
        {
          "name": "importer",
          "plugins":[
            {
              "name": "validate selection",
              "plugin": "importerTest"
            }
          ]
        },
        {
          "name": "post_import",
          "plugins":[
            {
              "name": "maya",
              "plugin": "postImportTest"
            }
          ]
        }
      ]
    }
  ],
  "finalizers": {
      "name": "finalizer",
      "plugins":[
        {
          "name": "to ftrack server",
          "plugin": "finalizerTest",
          "visible": false
        }
      ]
    }
}