{
  "type": "loader",
  "name": "Image Sequence Loader",
  "package": "image_sequence_unreal",
  "host_type": "unreal",
  "ui_type": "qt",
  "contexts": [
    {
      "name": "main",
      "stages": [
        {
          "name": "context",
          "plugins":[
            {
              "name": "context selector",
              "plugin": "common_default_loader_context",
              "widget": "common_default_loader_context"
            }
          ]
        }
      ]
    }
  ],
  "components": [
    {
      "name": "package_asset",
      "optional": true,
      "stages": [
        {
          "name": "collector",
          "plugins":[
            {
              "name": "Collect components from context",
              "plugin": "common_context_loader_collector",
              "options": {
                "accepted_formats": [".zip"]
              }
            }
          ]
        },
        {
          "name": "importer",
          "plugins":[
            {
              "name": "Import asset package",
              "widget": "load_image_sequence_unreal",
              "plugin": "asset_package_unreal_import"
            }
          ]
        },
        {
          "name": "post_import",
          "plugins":[
            {
              "name": "nuke",
              "plugin": "common_default_loader_post_import"
            }
          ]
        }
      ]
    }
  ],
  "finalizers": [
    {
      "name": "main",
      "stages": [
        {
          "name": "pre_finalizer",
          "plugins":[
            {
              "name": "pre to ftrack server",
              "plugin": "common_default_loader_pre_finalizer",
              "visible": false
            }
          ]
        },
        {
          "name": "finalizer",
          "plugins":[
            {
              "name": "Do nothing",
              "plugin": "common_default_loader_finalizer",
              "visible": false
            }
          ]
        },
        {
          "name": "post_finalizer",
          "plugins":[
            {
              "name": "post to ftrack server",
              "plugin": "common_default_loader_post_finalizer",
              "visible": false
            }
          ]
        }
      ]
    }
  ]
}